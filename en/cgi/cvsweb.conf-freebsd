# Set up for FreeBSD repo options.
#
# $FreeBSD$

$ENV{'RCSLOCALID'} = 'FreeBSD=CVSHeader';
$ENV{'RCSINCEXC'}  = 'iFreeBSD';
$ENV{'CVSROOT'}    = $cvsroot;

##############
# PR categoties
##############
@prcategories = qw(
		   advocacy
		   alpha
		   bin
		   conf
		   docs
		   gnu
		   i386
		   kern
		   misc
		   ports
		   sparc
		  );

##############
# query-pr.cgi
##############
$prcgi = "http://www.FreeBSD.org/cgi/query-pr.cgi?pr=";

